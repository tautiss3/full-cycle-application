<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Page Title</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" type="text/css" href="/stylesheets/tautvis-contacts-new.css">
  <link rel="stylesheet" href="https://cdn.linearicons.com/free/1.0.0/icon-font.min.css">
  <link rel="stylesheet" type="text/css" href="/stylesheets/tautvis-util.css">
</head>
<body>
    <% include ../_partials/header.ejs %>
      <!-- <% include ../_partials/navigation.ejs %> -->

    <div class="container-contact100">
        <div class="wrap-contact100">
          <form action="/contacts" method="POST" class="contact100-form validate-form">
            <span class="contact100-form-title">
              Send Us A Message
            </span>
    
            <label class="labelis" for="first-name">Tell us your name</label>
            <div class="wrap-input100 rs1-wrap-input100 validate-input" data-validate="Type your name">
              <input id="first-name" class="input100" type="text" name="name" placeholder="Your name" required autofocus>
              <span class="focus-input"></span>
            </div>

            <label class="labelis" for="email">Enter your email</label>
            <div class="wrap-input100 validate-input" data-validate = "Valid email is required: ex@abc.xyz">
              <input id="email" class="input100" type="email" name="email" placeholder="Eg. example@email.com" required autofocus>
              <span class="focus-input"></span>
            </div>

            <label class="labelis" for="email">Subject</label>
            <div class="wrap-input100 validate-input" data-validate = "Valid email is required: ex@abc.xyz">
              <input id="subject" class="input100" type="text" name="subject" placeholder="Subject title" required autofocus> 
              <span class="focus-input"></span>
            </div>
    
            <label class="labelis" for="message">Message</label>
            <div class="wrap-input100 validate-input" data-validate = "Message is required">
              <textarea id="message" class="input100" name="messages" placeholder="Write us a message" required autofocus></textarea>
              <span class="focus-input"></span>
            </div>
    
            <div class="container-contact100-form-btn">
              <button class="contact100-form-btn" >
                Send Message
              </button>
            </div>
          </form>
          <div class="contact100-more flex-col-c-m" style="background-image: url('images/bg-01.jpg');">
    
            <div class="dis-flex size1 p-b-47">
              <div class="flex-w size1 p-b-47">
                <div class="txt1 p-r-25">
                  <span class="lnr lnr-map-marker"></span>
                </div>
      
                <div class="flex-col size2">
                  <span class="txt1 p-b-20">
                    Address
                  </span>
      
                  <span class="txt2">
                    3th floor, Pamenkalnio g.5, Vilnius 01116, LT
                  </span>
                </div>
              </div>
              <div class="dis-flex size1 p-b-47">
                <div class="txt1 p-r-25">
                  <span class="lnr lnr-phone-handset"></span>
                </div>
      
                <div class="flex-col size2">
                  <span class="txt1 p-b-20">
                    Lets Talk
                  </span>
      
                  <span class="txt3">
                    +1 800 1236879
                  </span>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    
    
    

  <script src="/javascripts/tautvis-main.js"></script>
  <script src="/javascripts/contactUs.js"></script>
</body>
</html>